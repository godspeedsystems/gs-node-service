  "main": "index.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "start": "cd dist && node app.js",
    "clean": "rm -rf project build dist coverage",
    "lint": "eslint . --ext .ts --format table",
    "fix-lint": "eslint . --ext .ts --fix",
    "service-build": "mkdir -p build && cp -r src/* typings/* build/ && rsync -rv --delete --exclude='*.ts' build/ dist/ && rsync -rv --delete --exclude='*.ts' dist/ && npm install && ./node_modules/typescript/bin/tsc",
    "build": "mkdir -p build project/src && cp -r project/src/* src/* typings/* build/ && rsync -rv --delete --exclude='*.ts' build/ dist/ && rsync -rv --delete --exclude='*.ts' project/config dist/ && npm install && ./node_modules/typescript/bin/tsc",
    "prepare": "husky install",
    "commit-test": "NODE_CONFIG_DIR=./dist/tests/fixtures/config mocha -b ./dist/tests/*.test.js --timeout 15000",
    "test-one": "NODE_CONFIG_DIR=./dist/tests/fixtures/config mocha ./dist/tests/loadEvents.test.js --timeout 15000 | pino-pretty",
    "test": "NODE_CONFIG_DIR=./dist/tests/fixtures/config mocha ./dist/tests/*.test.js --timeout 15000 | pino-pretty",
    "test-cov": "nyc --reporter=html --reporter=text npm test"
  },
